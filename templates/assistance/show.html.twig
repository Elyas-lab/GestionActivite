{% extends 'base.html.twig' %}

{% block title %}Assistance{% endblock %}

{% block body %}
    <h1>Assistance</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ assistance.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ assistance.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ assistance.description }}</td>
            </tr>
            <tr>
                <th>Date_creation</th>
                <td>{{ assistance.dateCreation ? assistance.dateCreation|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_assistance_index') }}">back to list</a>

    <a href="{{ path('app_assistance_edit', {'id': assistance.id}) }}">edit</a>

    {{ include('assistance/_delete_form.html.twig') }}
{% endblock %}
