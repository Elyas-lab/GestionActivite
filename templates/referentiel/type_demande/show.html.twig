{% extends 'inside.html.twig' %}

{% block title %}TypeDemande Détails{% endblock %}

{% block main %}
    <h1>TypeDemande Details</h1>

    <table class="table">
        <tr>
            <th>Numéro</th>
            <td>{{ type_demande.id }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ type_demande.nom }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ type_demande.description }}</td>
        </tr>
    </table>

    <a href="{{ path('app_type_edit', {'id': type_demande.id}) }}" class="btn btn-warning">Modifier</a>
    <a href="{{ path('app_type_index') }}" class="btn btn-secondary">Retour</a>
{% endblock %}
